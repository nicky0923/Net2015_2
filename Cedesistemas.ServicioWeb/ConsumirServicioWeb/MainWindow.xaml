<Window x:Class="ConsumirServicioWeb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,2,-3">
        <Button x:Name="BtnConsumir" Content="Consumir servicio" HorizontalAlignment="Left" Margin="172,47,0,0" VerticalAlignment="Top" Width="174" Click="BtnConsumir_Click"/>
        <DataGrid x:Name="DataGridPersonas" Margin="0,132,37,0" VerticalAlignment="Top" Height="164" HorizontalAlignment="Right" Width="462">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Nombre" Binding="{Binding Nombre}" />
                <DataGridTextColumn Header="Apellidos" Binding="{Binding Apellidos}" />
                <DataGridTextColumn Header="FechaNacimiento" Binding="{Binding FechaNacimiento}" />
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
